{
  "name": "agents",
  "sourceRoot": "apps/agents/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --loader ts-node/esm apps/agents/src/main.ts"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/agents/tsconfig.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'lint placeholder'",
        "cwd": "apps/agents"
      }
    }
  }
}