{
  "name": "agents-lib",
  "sourceRoot": "libs/agents/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/agents",
        "main": "libs/agents/src/index.ts",
        "tsConfig": "libs/agents/tsconfig.lib.json"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/agents/**/*.ts"]
      }
    }
  }
}
