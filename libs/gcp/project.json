{
  "name": "gcp",
  "sourceRoot": "libs/gcp/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/gcp",
        "main": "libs/gcp/src/index.ts",
        "tsConfig": "libs/gcp/tsconfig.lib.json"
