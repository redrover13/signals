    "lint:ci": "nx affected --target=lint --parallel=3 --max-warnings=0",
    "affected": "nx affected --target=build",
    "env:check": "tsx ./tools/env/check.ts",
    "env:print": "tsx ./tools/env/print.ts",
    "mcp:demo": "tsx examples/mcp-demo.ts",
    "mcp:build": "nx build mcp",
    "mcp:test": "nx test mcp --testTimeout=30000",
    "mcp:health": "tsx -e \"import('@dulce/mcp').then(m => m.testMCPConnectivity().then(console.log))\"",
