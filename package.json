    "ts:build:watch": "nx run-many --target=build --all --watch",
    "ts:build:clean": "tsc -b tsconfig.references.json --clean",
    "ts:check": "nx run-many --target=typecheck --all --skip-nx-cache",
    "ts:check:strict": "tsc --noEmit --strict",
    "ts:refs:enable": "node scripts/enable-ts-project-references.js",
    "ts:analyze": "node ./tools/scripts/typescript-strict-analyzer.js",
    "ts:fix": "node ./tools/scripts/typescript-strict-fixer.js",
